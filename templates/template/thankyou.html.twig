{% extends 'base.html.twig' %}

{% block title %}Kindergarten - Daycare and Kindergarten Website Template{% endblock %}


{% block javascripts %}
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-ENjdO4Dr2bkBIFxQpeoTz1HIcje39Wm4jDKdf19U8gI4ddQ3GYNS7NTKfAdVQSZe"
            crossorigin="anonymous"></script>

{% endblock %}


{% block body %}

    <section id="thank-you">
        <div class="container py-5 my-5 ">
            <div class="row justify-content-center">

                <div class="col-md-12">
                    <div class="contact-information">

                        <div class="section-header">
                            <h2>Get in Touch</h2>
                            <p>We will get back to you as soon as possible.</p>
                        </div>
                        <div class="row mt-5">
                            <div class="d-flex flex-wrap">
                                <div class="col-md-6 mb-5">
                                    <div class="detail">
                                        <h2>Phones</h2>
                                        <ul class="list-unstyled">
                                            <li>
                                                <iconify-icon class="text-primary fs-5 me-2" icon="solar:phone-bold"
                                                              style="vertical-align:text-bottom"></iconify-icon> +1650-243-00023
                                            </li>
                                            <li>
                                                <iconify-icon class="text-primary fs-5 me-2" icon="solar:phone-bold"
                                                              style="vertical-align:text-bottom"></iconify-icon> +1650-243-00021
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                                <div class="col-md-6 mb-5 ">
                                    <div class="detail">
                                        <h2>Emails</h2>
                                        <ul class="list-unstyled">
                                            <li>
                                                <iconify-icon class="text-primary fs-5 me-2" icon="ic:baseline-email"
                                                              style="vertical-align:text-bottom"></iconify-icon> <a href="#">info@yourcompany.com</a>
                                            </li>
                                            <li>
                                                <iconify-icon class="text-primary fs-5 me-2" icon="ic:baseline-email"
                                                              style="vertical-align:text-bottom"></iconify-icon> <a href="#">support@yourcompany.com</a>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                                <div class="col-md-6 mb-5 ">
                                    <div class="address detail">
                                        <h2>Address</h2>
                                        <ul class="list-unstyled">
                                            <li>
                                                <iconify-icon class="text-primary fs-5 me-2" icon="mdi:location"
                                                              style="vertical-align:text-bottom"></iconify-icon> <span>North Melbourne VIC 3051,
                          Australia</span>
                                            </li>
                                            <li>
                                                <iconify-icon class="text-primary fs-5 me-2" icon="mdi:location"
                                                              style="vertical-align:text-bottom"></iconify-icon> <span>South Sydney VIC 2045,
                          Australia</span>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                                <div class="col-md-6 mb-5">
                                    <div class="detail">
                                        <h2>Social Links</h2>
                                        <ul class="social-links list-unstyled d-flex">
                                            <li><a href="#" class="me-3"><iconify-icon class="social-icon fs-5"
                                                                                       icon="ri:facebook-fill"></iconify-icon></a></li>
                                            <li><a href="#" class="me-3"><iconify-icon class="social-icon fs-5"
                                                                                       icon="ri:twitter-fill"></iconify-icon></a></li>
                                            <li><a href="#" class="me-3"><iconify-icon class="social-icon fs-5"
                                                                                       icon="ri:pinterest-fill"></iconify-icon></a></li>
                                            <li><a href="#" class="me-3"><iconify-icon class="social-icon fs-5"
                                                                                       icon="ri:instagram-fill"></iconify-icon></a></li>
                                            <li><a href="#" class="me-3"><iconify-icon class="social-icon fs-5"
                                                                                       icon="ri:youtube-fill"></iconify-icon></a></li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>

                </div>
            </div>
        </div>

    </section>

{% endblock %}
